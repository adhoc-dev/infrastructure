        <act_window id="launch_session_deployvm"
                    name="Deploy VM Instance"
                    src_model="cloudmanager.server"
                    res_model="cloudmanager.deployvm"
                    view_mode="form"
                    target="new"
                    key2="client_action_multi"/>
    </data>
</openerp>
